{
  "name": "@cloud-mobile/appium-flutter-driver-iosv14plus",
  "description": "Appium Flutter Driver iOS 14+",
  "keywords": [
    "appium",
    "flutter"
  ],
  "version": "1.7.2",
  "main": "./build/driver/lib/driver.js",
  "appium": {
    "driverName": "flutter-ios14plus",
    "automationName": "Flutter-iOS14Plus",
    "platformNames": [
      "iOS",
      "Android"
    ],
    "mainClass": "FlutterDriver"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "prepublishOnly": "cp ../README.md ../LICENSE ./",
    "prepare": "npm run clean && npm run compile",
    "test": "echo no test",
    "compile": "tsc && mkdir ./build/driver/lib/est-code && cp ./lib/est-code/appium-xcuitest-driver.zip ./build/driver/lib/est-code/appium-xcuitest-driver.zip",
    "clean": "rm -rf build",
    "lint": "tslint 'lib/**/*.ts'",
    "clean-dependency": "rm -rf node_modules && rm -f package-lock.json && npm install",
    "postinstall": "unzip -o ./build/driver/lib/est-code/appium-xcuitest-driver.zip -d ./node_modules/appium-xcuitest-driver/build/lib"
  },
  "peerDependencies": {
    "appium": "^2.0.0-beta.40"
  },
  "devDependencies": {
    "@appium/schema": "^0.0.8",
    "@appium/types": "^0.4.0",
    "@wdio/types": "^7.20.0",
    "json-schema": "^0.4.0",
    "tslint": "^6.1.3",
    "type-fest": "^3.0.0",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "appium-adb": "^9.9.0",
    "appium-android-driver": "^5.2.2",
    "appium-ios-device": "^2.4.1",
    "appium-uiautomator2-driver": "^2.4.1",
    "appium-xcuitest-driver": "^4.7.1",
    "asyncbox": "^2.3.1",
    "bluebird": "^3.1.1",
    "lodash": "^4.0.0",
    "portscanner": "2.2.0",
    "rpc-websockets": "^7.4.16"
  }
}
